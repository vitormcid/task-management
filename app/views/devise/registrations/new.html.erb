<%= render "style" %>
<div class="login-page">
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <div class="form">      
      <form class="login-form">    
        <%= devise_error_messages! %> 
        <%= f.email_field :email, autofocus: true,value: "email" ,class: "form-control" %>
        <%= f.text_field :username, value: "username", class: "form-control" %>  
        <% if @minimum_password_length %>
        <% end %><br />
        <%= f.label "Senha" %><br />
        <%= f.password_field :password,value: "password", autocomplete: "off", class: "form-control" %>
        <%= f.label "Confirmação da senha"%><br />
        <%= f.password_field :password_confirmation,value:"password" ,autocomplete: "off", class: "form-control" %>       
       <%= f.submit "Sign up", class: "btn btn-dark" %>
      <%= render "devise/shared/links" %>
      </form>
    </div>
  <% end %>  
</div>
